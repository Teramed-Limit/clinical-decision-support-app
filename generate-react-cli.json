{
	"usesTypeScript": true,
	"usesCssModule": true,
	"cssPreprocessor": "scss",
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"customTemplates": {
				"component": "src/templates/component/TemplateName.tsx",
				"style": "src/templates/component/TemplateName.module.scss"
			},
			"path": "src/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		},
		"container": {
			"customTemplates": {
				"component": "src/templates/component/TemplateName.tsx",
				"style": "src/templates/component/TemplateName.module.scss"
			},
			"path": "src/container",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		},
		"ui": {
			"customTemplates": {
				"component": "src/templates/component/TemplateName.tsx",
				"style": "src/templates/component/TemplateName.module.scss"
			},
			"path": "src/components/UI",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		},
		"report-field": {
			"customTemplates": {
				"component": "src/templates/component/TemplateName.tsx",
				"style": "src/templates/component/TemplateName.module.scss"
			},
			"path": "src/container/Report/field",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		}
	}
}

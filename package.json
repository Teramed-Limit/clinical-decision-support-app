{
	"name": "clinical-decision-support-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint": "yarn lint:format && yarn lint:fix ",
		"type-check": "tsc",
		"gc": "npx generate-react-cli component",
		"jsonserver": "json-server --watch src/fake-data/db.json --routes src/fake-data/routes.json --port 3004"
	},
	"dependencies": {
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@fontsource/roboto": "^4.5.8",
		"@mui/icons-material": "^5.11.16",
		"@mui/material": "^5.12.2",
		"ag-grid-community": "^29.3.3",
		"ag-grid-react": "^29.3.3",
		"axios": "^1.3.6",
		"date-fns": "^2.29.3",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"ramda": "^0.29.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.10.0",
		"recharts": "^2.5.0",
		"recoil": "^0.7.7",
		"rxjs": "^7.8.0",
		"sass": "^1.62.0",
		"sort-by": "^1.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitejs/plugin-react": "^4.0.0-beta.0",
		"eslint": "^8.38.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.3.4",
		"prettier": "^2.8.8",
		"prettier-eslint": "^15.0.1",
		"typescript": "4.7.4",
		"vite": "^4.3.0",
		"vite-plugin-rewrite-all": "^1.0.1"
	}
}
